  data, scale = "none",
  dendrogram = "none", Rowv = NA, Colv = NA,
  col = bluered(256),
  breaks=seq(0.45,0.75,length.out=257),
  trace = "none",
  density.info=c("none"),
  main = "Correlaton Coefficient"
)

# Fig.5d
proteome <- read.table("Data_Fig5d.txt", header=F)
data <- as.matrix(proteome)
library(gplots)
my.col1 <- colorRampPalette(c("white","yellow","red","brown")) 
heatmap.2(
  data, scale = "none",
  dendrogram = "none", Rowv = NA, Colv = NA,
  col = my.col1(256),
  breaks=seq(0,0.8,length.out=257),
  trace = "none",
  density.info=c("none"),
  main = "Spearman's CC"
)

