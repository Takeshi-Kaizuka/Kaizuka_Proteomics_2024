axis(side=1,at=seq(-5e+1,2e+1,1e+1),tck=1.0,lty="dotted",lwd=0.5,col="#dddddd",labels=expression(-5e+1,-4e+1,-3e+1,-2e+1,-1e+1,0,1e+1,2e+1))
axis(side=2,at=seq(-3e+1,3e+1,1e+1),tck=1.0,lty="dotted",lwd=0.5,col="#dddddd",labels=expression(-3e+1,-2e+1,-1e+1,0,1e+1,2e+1,3e+1))
points(x=rpca$x[1,1],y=rpca$x[1,2],pch=21,col="black",bg="blue")
points(x=rpca$x[2,1],y=rpca$x[2,2],pch=22,col="black",bg="blue")
points(x=rpca$x[3,1],y=rpca$x[3,2],pch=21,col="black",bg="green")
points(x=rpca$x[4,1],y=rpca$x[4,2],pch=22,col="black",bg="green")
points(x=rpca$x[5,1],y=rpca$x[5,2],pch=21,col="black",bg="yellow")
points(x=rpca$x[6,1],y=rpca$x[6,2],pch=22,col="black",bg="yellow")
points(x=rpca$x[7,1],y=rpca$x[7,2],pch=21,col="black",bg="orange")
points(x=rpca$x[8,1],y=rpca$x[8,2],pch=22,col="black",bg="orange")
points(x=rpca$x[9,1],y=rpca$x[9,2],pch=21,col="black",bg="red")
points(x=rpca$x[10,1],y=rpca$x[10,2],pch=22,col="black",bg="red")
box(bty="l")
summary(rpca)$importance

